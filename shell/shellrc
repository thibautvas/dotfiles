export HOST_PROJECT_DIR="$HOME/repos"
export HOST_COLOR='MAGENTA'
export PICKER_CMD='FzfLua files';

[[ -f "$HOME/.config/shell/prompt.sh" ]] && source "$HOME/.config/shell/prompt.sh"
[[ -f "$HOME/.config/shell/aliases.sh" ]] && source "$HOME/.config/shell/aliases.sh"

[[ -f "$HOME/.config/tmux/bin/tls" ]] && alias tls="$HOME/.config/tmux/bin/tls"
